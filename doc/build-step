sudo apt-get install build-essential libxrender-dev xorg-dev libasound2-dev libcups2-dev gawk zip libxtst-dev libxi-dev libxt-dev gobjc

bash ./configure --with-target-bits=64 --with-num-cores=2 --with-debug-level=slowdebug --with-boot-jdk=/home/linmm/devworks/jdk/jdk1.7.0_80 --enable-debug-symbols ZIP_DEBUGINFO_FILES=0


